<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>rest-kit-loxal</application>
    <version>1</version>

    <ssl-enabled>true</ssl-enabled>
    <sessions-enabled>false</sessions-enabled>
    <precompilation-enabled>false</precompilation-enabled>
    <threadsafe>false</threadsafe>

    <admin-console>
        <page name="Custom" url="/"/>
    </admin-console>

    <inbound-services>
        <service>warmup</service>
    </inbound-services>

    <static-files>
        <include path="**/*.txt"/>
        <include path="**/*.gif"/>
        <include path="**/*.png"/>
        <include path="**/*.ico"/>
        <include path="**/*.html"/>
        <include path="**/*.css"/>
        <include path="**/*.js"/>
        <include path="**/*.jpg"/>
        <include path="**/*.pdf"/>
        <include path="**/*"/>
        <include path="*"/>
    </static-files>

    <system-properties>
        <property name="admin.name" value="Alexander Orlov"/>
        <property name="admin.mail" value="alexander.orlov@loxal.net"/>
    </system-properties>

    <env-variables>
        <env-var name="DEFAULT_ENCODING" value="UTF-8"/>
    </env-variables>
</appengine-web-app>