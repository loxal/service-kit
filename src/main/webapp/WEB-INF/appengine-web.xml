<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <vm>true</vm>

    <vm-settings>
        <setting name="machine_type" value="g1-small"/>
    </vm-settings>

    <manual-scaling>
        <instances>1</instances>
    </manual-scaling>

    <application>rest-kit-loxal</application>
    <version>1</version>
    <ssl-enabled>true</ssl-enabled>
    <sessions-enabled>false</sessions-enabled>
    <precompilation-enabled>false</precompilation-enabled>
    <threadsafe>true</threadsafe>

    <admin-console>
        <page name="Custom" url="/"/>
    </admin-console>

    <inbound-services>
        <service>warmup</service>
    </inbound-services>

    <system-properties>
        <property name="admin.name" value="Alexander Orlov"/>
        <property name="admin.email" value="alexander.orlov@loxal.net"/>
    </system-properties>

    <env-variables>
    </env-variables>
</appengine-web-app>